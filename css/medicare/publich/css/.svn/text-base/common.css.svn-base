/* checkbox*/
	.check-box {
		display: inline-block;
	}
	
	.check-box label {
		height: 20px;
		margin: 0px;
		padding-left: 0px;
	}
	.check-box label.check {
		
	}
	.check-box input {
		position: absolute;
		opacity: 0;
		margin-left: 0px!important;
	}
	.check-box i {
		transition: color 0.25s linear;
		position: absolute;
	}
	.check-box i:before{
			content: "\f096";
			color: #bdc3c7;
	}
	
	.check-box label:hover i:before{
      content: "\f046";
   }
   .check-box label.checked i:before {
   	color: #1abc9c;
   	content: "\f046";
   }
	
	.check-box label.checked span  {
		color: #1abc9c;
	}
	
	.check-box span.text {
		margin-left: 20px;
	}
